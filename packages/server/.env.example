# Server Configuration
PORT=3000
NODE_ENV=development

# Database
DATABASE_URL="file:./dev.db"

# API Keys (generate secure random strings in production)
CLIENT_API_KEY=client_secret_key_here
AGENCY_API_KEY=agency_secret_key_here

# Admin API Secret (for managing API keys)
ADMIN_SECRET=your_admin_secret_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
CORS_ORIGIN=*
